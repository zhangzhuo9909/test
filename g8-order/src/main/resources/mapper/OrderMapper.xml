<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zhangzhuo.mapper.OrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.zhangzhuo.entity.Order">
        <id column="id" property="id" />
        <result column="member_id" property="memberId" />
        <result column="status" property="status" />
        <result column="ptotal" property="ptotal" />
        <result column="fee" property="fee" />
        <result column="amount" property="amount" />
        <result column="reduce_money" property="reduceMoney" />
        <result column="final_pay_money" property="finalPayMoney" />
        <result column="score" property="score" />
        <result column="remark" property="remark" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, member_id, status, ptotal, fee, amount, reduce_money, final_pay_money, score, remark, gmt_create, gmt_modified
    </sql>

</mapper>
